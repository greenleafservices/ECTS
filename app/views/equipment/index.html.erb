<p id="notice"><%= notice %></p>

<h1>Equipment</h1>

<table>
  <thead>
    <tr>
      <th>EquipID</th>
      <th>Cust</th>
      <th>Equip</th>
      <th>Csa</th>
      <th>Serial</th>
      <th>Model</th>
      <th>Manufacturer</th>
      <th>Product type</th>
      <th>Date installed</th>
      <th>Customer</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @equipment.each do |equipment| %>
      <tr>
        <td><%= equipment.id %></td>
        <td><%= equipment.cust_id %></td>
        <td><%= equipment.equip_id %></td>
        <td><%= equipment.csa %></td>
        <td><%= equipment.serial %></td>
        <td><%= equipment.model %></td>
        <td><%= equipment.manufacturer %></td>
        <td><%= equipment.product_type %></td>
        <td><%= equipment.date_installed %></td>
        <td><%= equipment.customer_id %></td>
        <td><%= link_to 'Show', equipment %></td>
        <td><%= link_to 'Edit', edit_equipment_path(equipment) %></td>
        <td><%= link_to 'Destroy', equipment, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Equipment', new_equipment_path %>
